{
  "outputDirectory": "docs",
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/archive/:path*",
      "has": [
        {
          "type": "host",
          "value": "archive.evazhang.com"
        }
      ]
    },
    {
      "source": "/archive/:path*",
      "destination": "/archive/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/:path*"
    }
  ]
}
